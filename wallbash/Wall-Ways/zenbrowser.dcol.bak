$HOME/.zen/'tsgtn71d.Default (alpha)'/chrome/userChrome.css
:root {
    --zen-colors-primary: #<wallbash_1xa1> !important;
    --zen-colors-secondary: #<wallbash_2xa1> !important;
    --zen-colors-tertiary: #<wallbash_3xa1> !important;
    --zen-colors-border: #<wallbash_1xa1> !important;
    --zen-primary-color: #<wallbash_txt1> !important; 
    --zen-dialog-background: #<wallbash_1xa1> !important;
}

#zen-main-app-wrapper,
#TabsToolbar, 
#toolbar-menubar, 
#main-menubar, 
#titlebar,
#tabbrowser-tabpanels{
    background-color: var(--zen-colors-primary) !important;
}

#appMenu-mainView {
    background-color: var(--zen-colors-secondary) !important;
}

#appmenu-moreTools {
    background-color: var(--zen-colors-secondary) !important;
}

#PanelUI-button {
    --button-active-bgcolor: var(--zen-colors-primary) !important;
}

toolbox#navigator-toolbox {
    background-color: var(--zen-colors-primary) !important;
}

#zen-appcontent-navbar-container {
    background-color: var(--zen-colors-primary) !important;
}

.menu-text {
    color: var(--zen-primary-color) !important;
}

.menuitem-iconic {
    color: var(--zen-primary-color) !important;
}

:not(:not(menubar) > menu, #ContentSelectDropdown) > menupopup > menuitem:not(.menuitem-iconic, [type="checkbox"], [type="radio"], .in-menulist, .in-menulist menuitem, .unified-nav-current), :not(:not(menubar) > menu, #ContentSelectDropdown) > menupopup > menu:not(.menu-iconic, [type="checkbox"], [type="radio"], .in-menulist, .in-menulist menu, .unified-nav-current), #toggle_toolbar-menubar, #PanelUI-history toolbarbutton, #unified-extensions-context-menu menuitem {
    fill: var(--zen-primary-color) !important;
}

html#main-window {
    --lwt-text-color: var(--zen-primary-color) !important;
    --toolbarbutton-icon-fill: var(--zen-primary-color) !important;
    --toolbar-field-color: var(--zen-primary-color) !important;
    --toolbar-field-focus-color: var(--zen-primary-color) !important;
    --toolbar-field-background-color: var(--zen-colors-tertiary) !important;
    --toolbar-field-focus-background-color: var(--zen-colors-tertiary) !important;
    
    --zen-colors-primary: #<wallbash_1xa1> !important;
    --zen-colors-secondary: #<wallbash_2xa1> !important;
    --zen-colors-tertiary: #<wallbash_3xa1> !important;
    --zen-colors-border: #<wallbash_1xa1> !important;
    --zen-dialog-background: #<wallbash_1xa1> !important;
    
    
    
    --arrowpanel-color: var(--zen-colors-primary) !important;
}
